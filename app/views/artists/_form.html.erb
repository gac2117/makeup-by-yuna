<%= form_for @artist do |f| %>
  <%= f.label 'Name' %><br>
  <%= f.text_field :name %><br><br>

  <%= f.label 'Email Address' %><br>
  <%= f.email_field :email %><br><br>

  <%= f.label 'Short Bio' %><br>
  <%= f.text_field :extra %><br><br>

  <%= f.label 'Password' %><br>
  <%= f.password_field :password %><br><br>

  <%= f.label 'What is the secret code?' %><br>
  <%= f.text_field :secret %><br><br>

  <%= f.submit "Submit" %>
<% end %>
